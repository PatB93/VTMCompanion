<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_marginStart="5dp"
    android:layout_width="wrap_content"
    android:layout_height="20dp">

    <com.google.android.material.checkbox.MaterialCheckBox
        style="@style/AppTheme.Checkbox"
        android:id="@+id/dot_one"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.checkbox.MaterialCheckBox
        style="@style/AppTheme.Checkbox"
        android:id="@+id/dot_two"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/dot_one"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.checkbox.MaterialCheckBox
        style="@style/AppTheme.Checkbox"
        android:id="@+id/dot_three"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/dot_two"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@drawable/filled_dot" />

    <com.google.android.material.checkbox.MaterialCheckBox
        style="@style/AppTheme.Checkbox"
        android:id="@+id/dot_four"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/dot_three"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@drawable/filled_dot" />

    <com.google.android.material.checkbox.MaterialCheckBox
        style="@style/AppTheme.Checkbox"
        android:id="@+id/dot_five"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/dot_four"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:src="@drawable/empty_dot" />

</androidx.constraintlayout.widget.ConstraintLayout>